<Window x:Class="CharacterSheet.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CharacterSheet"
        mc:Ignorable="d"
        Title="MainWindow" Height="900" Width="850">
    <Grid>
        <TextBox x:Name="CharacterNameField" HorizontalAlignment="Left" Margin="47,29,0,0" TextWrapping="Wrap" Text="Name" VerticalAlignment="Top" Width="203" Height="40" FontSize="22"/>
        <Label x:Name="CharacterNameLabel" Content="Character Name" HorizontalAlignment="Left" Margin="47,69,0,0" VerticalAlignment="Top" Height="27" Width="101"/>
    </Grid>
</Window>
